    <connection>CiatPolicies</connection>
    <sql>SELECT distinct
  pin.id policy_info_id
, pin.project_policy_id
, pin.year
, pin.title
, pin.description
, pin.global_unit_id
, po.is_active
FROM policies_ciat.project_policy_info pin, policies_ciat.project_policies po
where pin.project_policy_id = po.id
and pin.year in (2018,2019)
order by pin.global_unit_id;
</sql>
